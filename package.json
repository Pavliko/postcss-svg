    "test-sprite-module": "file:test/sprite-module"
  },
  "eslintConfig": {
    "extends": "dev"
  },
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "svgs",
    "scalable",
    "vector",
    "graphic",
